-------------------------------- MODULE Edn --------------------------------

ToEdn(value) ==
  (*************************************************************************)
  (* Converts the given value to a EDN string.                             *)
  (*************************************************************************)
  TRUE

EdnSerialize(absoluteFilename, value) ==
  (*************************************************************************)
  (* Serializes a TLC value to the given file as EDN.                      *)  
  (*************************************************************************)
  TRUE

EdnDeserialize(absoluteFilename) ==
  (*************************************************************************)
  (* Deserializes EDN values from the given file.                          *)
  (*************************************************************************)
  CHOOSE val : TRUE

=============================================================================
